  <xs:complexType name="CandidatePersonType">
    <xs:annotation>
      <xs:documentation>A collection of demographic and personal data about a candidate -- a person being considered for, or who has otherwise been associated with, a position opening or resource requirement.</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element minOccurs="0" maxOccurs="1" name="legalId" type="hros:IdentifierType" />
      <xs:element minOccurs="0" maxOccurs="1" name="birthDate" type="hros:DateType" />
      <xs:element minOccurs="0" maxOccurs="1" name="gender" type="hros:GenderCodeList" />
      <xs:element minOccurs="0" maxOccurs="1" name="citizenship">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:CountryCodeList" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="residenceCountry">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:CountryCodeList" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="militaryStatus">
        <xs:simpleType>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="ethnicity">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item">
              <xs:simpleType>
                <xs:restriction base="xs:string" />
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="id" type="hros:IdentifierType" />
      <xs:element minOccurs="0" maxOccurs="1" name="name" type="hros:PersonNameType" />
      <xs:element minOccurs="0" maxOccurs="1" name="communication" type="hros:CommunicationType" />
      <xs:element minOccurs="0" maxOccurs="1" name="primaryLanguage" type="hros:LanguageCodeList" />
      <xs:element minOccurs="0" maxOccurs="1" name="height" type="hros:MeasureType" />
      <xs:element minOccurs="0" maxOccurs="1" name="weight" type="hros:MeasureType" />
      <xs:element minOccurs="0" maxOccurs="1" name="disability" type="hros:DisabilityType" />
      <xs:element minOccurs="0" maxOccurs="1" name="tobaccoUser">
        <xs:simpleType>
          <xs:restriction base="xs:boolean" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="hairColor">
        <xs:simpleType>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="eyeColor">
        <xs:simpleType>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="identifyingMarks" type="hros:StringTypeArray" />
      <xs:element minOccurs="0" maxOccurs="1" name="age">
        <xs:simpleType>
          <xs:restriction base="xs:integer" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="bloodType" type="hros:CodeType" />
      <xs:element minOccurs="0" maxOccurs="1" name="passportId" type="hros:IdentifierType" />
      <xs:element minOccurs="0" maxOccurs="1" name="fatherName">
        <xs:simpleType>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="motherName">
        <xs:simpleType>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="race">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item">
              <xs:simpleType>
                <xs:restriction base="xs:string" />
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="birthPlace">
        <xs:simpleType>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="deathDate" type="hros:DateTimeType" />
      <xs:element minOccurs="0" maxOccurs="1" name="nationality">
        <xs:simpleType>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="maritalStatus">
        <xs:simpleType>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="visa">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item">
              <xs:complexType>
                <xs:all />
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="religion">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item">
              <xs:simpleType>
                <xs:restriction base="xs:string" />
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="studentType" type="hros:CodeType" />
      <xs:element minOccurs="0" maxOccurs="1" name="studentIndicator" type="hros:IndicatorType" />
      <xs:element minOccurs="0" maxOccurs="1" name="studentStatus" type="hros:CodeType" />
    </xs:all>
  </xs:complexType>
  <xs:complexType name="CandidateProfileType">
    <xs:annotation>
      <xs:documentation>Contains structured information about a candidate, including information about their past experiences (Work History, Education History, etc). This information can be used for evaluation or submission of candidacy for a position opening or resource requirement.</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element minOccurs="0" maxOccurs="1" name="languageCode" type="hros:LanguageCodeList">
        <xs:annotation>
          <xs:documentation>The primary language of the content of the profile.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="id" type="hros:IdentifierType">
        <xs:annotation>
          <xs:documentation>The primary identifier of the candidate profile. This should be the identifier assigned by the owner of the data.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="alternateIds">
        <xs:complexType>
          <xs:annotation>
            <xs:documentation>An array of other identifiers for the candidate profile. These may be identifiers assigned by other trading partners or third party systems.</xs:documentation>
          </xs:annotation>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:IdentifierType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="profileName">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>The name given to the profile.</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="personAvailability" type="hros:PersonAvailabilityType">
        <xs:annotation>
          <xs:documentation>The availability of this profile to be considerd for work assignments.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="processingInformation">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Instructions to a third party on how this profile should be processed.</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="distributionGuidelines" type="hros:DistributionGuidelinesType">
        <xs:annotation>
          <xs:documentation>Instructions on how this profile should be distributed to third parties.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="associatedPositionOpenings">
        <xs:complexType>
          <xs:annotation>
            <xs:documentation>The positions in which this candidate is associated. This could be due to application or source activities.</xs:documentation>
          </xs:annotation>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:AssociatedPositionOpeningType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="objective">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>The objective of the candidate in finding appropriate employment. A brief statement on career and position goals.</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="executiveSummary">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>A concice section describing the experience of the candidate, or an introduction to the candidate.</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="employerPreferences">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:EmployerPreferenceType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="positionPreferences">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:PositionPreferenceType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="employmentHistory" type="hros:EmploymentHistoryType" />
      <xs:element minOccurs="0" maxOccurs="1" name="educationHistory" type="hros:EducationHistoryType" />
      <xs:element minOccurs="0" maxOccurs="1" name="licenses">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:LicenseType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="certifications">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:CertificationType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="militaryHistory" type="hros:MilitaryHistoryType" />
      <xs:element minOccurs="0" maxOccurs="1" name="patents">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:PatentType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="publications">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:PublicationType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="organizationAffiliations">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:OrganizationAffiliationType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="employmentReferences">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:RefereeType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="attachments">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:AttachmentType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="PersonAvailabilityType">
    <xs:annotation>
      <xs:documentation>Details about when a person is available for work engagements.</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element minOccurs="0" maxOccurs="1" name="availabilityDates">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:AvailabilityDatesType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="noticePeriodMeasure" type="hros:MeasureType" />
      <xs:element minOccurs="0" maxOccurs="1" name="immediateStartIndicator" type="hros:IndicatorType" />
    </xs:all>
  </xs:complexType>
  <xs:complexType name="AvailabilityDatesType">
    <xs:annotation>
      <xs:documentation>The dates of availability for work engagements.</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element minOccurs="0" maxOccurs="1" name="startDateTime" type="hros:FormattedDateTimeType" />
      <xs:element minOccurs="0" maxOccurs="1" name="endDateTime" type="hros:FormattedDateTimeType" />
    </xs:all>
  </xs:complexType>
  <xs:complexType name="DistributionGuidelinesType">
    <xs:annotation>
      <xs:documentation>The rules for dispersing information contained in the object to a person or organization.</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element minOccurs="0" maxOccurs="1" name="doNotRedistributeIndicator" type="hros:IndicatorType" />
      <xs:element minOccurs="0" maxOccurs="1" name="distributeTo">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:DistributeToType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="doNotDistributeTo">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:DoNotDistributeToType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="DoNotDistributeToType">
    <xs:annotation>
      <xs:documentation>The person or organization to which the objects should not be delivered (excluded), including the effective dates to this exclusion.</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element minOccurs="0" maxOccurs="1" name="startDate" type="hros:FormattedDateTimeType" />
      <xs:element minOccurs="0" maxOccurs="1" name="endDate" type="hros:FormattedDateTimeType" />
      <xs:element minOccurs="0" maxOccurs="1" name="duration" type="hros:DurationType" />
      <xs:element minOccurs="0" maxOccurs="1" name="person" type="hros:EntityType" />
      <xs:element minOccurs="0" maxOccurs="1" name="organization" type="hros:OrganizationType" />
      <xs:element minOccurs="0" maxOccurs="1" name="communication" type="hros:CommunicationType" />
      <xs:element minOccurs="0" maxOccurs="1" name="descriptions" type="hros:StringTypeArray" />
    </xs:all>
  </xs:complexType>
  <xs:complexType name="DistributeToType">
    <xs:annotation>
      <xs:documentation>The person or organization to which the objects should be delivered, and the effective dates of this distribution.</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element minOccurs="0" maxOccurs="1" name="startDate" type="hros:FormattedDateTimeType" />
      <xs:element minOccurs="0" maxOccurs="1" name="endDate" type="hros:FormattedDateTimeType" />
      <xs:element minOccurs="0" maxOccurs="1" name="duration" type="hros:DurationType" />
      <xs:element minOccurs="0" maxOccurs="1" name="person" type="hros:EntityType" />
      <xs:element minOccurs="0" maxOccurs="1" name="organization" type="hros:OrganizationType" />
      <xs:element minOccurs="0" maxOccurs="1" name="communication" type="hros:CommunicationType" />
      <xs:element minOccurs="0" maxOccurs="1" name="descriptions" type="hros:StringTypeArray" />
    </xs:all>
  </xs:complexType>
  <xs:complexType name="AssociatedPositionOpeningType">
    <xs:annotation>
      <xs:documentation>Contains details answering the questions: 1. What, if any, are the positions openings with which is the Candidate associated? 2. How did the Candidate become associated with those positions? The positionOpeningId, positiontitle, and positionUri components address the first question. The searchCriteria, searchResult, and candidateAppliedIndicator address the second question. All elements are optional.  In general, a candidate becomes associated with a position by applying to be considered for the position or by having been sourced and matched to the opening through some manner of automated and/or human-initiated recruiting or research process.</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element minOccurs="0" maxOccurs="1" name="positionOpeningId" type="hros:IdentifierType">
        <xs:annotation>
          <xs:documentation>An identifier to a related position opening (related to recruiting for a job).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="staffingOrderId" type="hros:IdentifierType" />
      <xs:element minOccurs="0" maxOccurs="1" name="positionTitle">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>A short phrase describing the position as it would be listed on a business card or in a company directory. OED: An appellation attaching to a position in virtue of rank, function or office.</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="positionUri">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>A Uniform Resource Identifier to the associated position opening. This is often a web page or HTTP URL.</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:anyURI" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="searchCriteria">
        <xs:complexType>
          <xs:annotation>
            <xs:documentation>The associated search criteria used by the candidate or recruiter to match this candidate to the associated position opening.</xs:documentation>
          </xs:annotation>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:SearchCriterionType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="searchResult" type="hros:SearchResultType">
        <xs:annotation>
          <xs:documentation>The associated search result matching the criteria used by the candidate or recruiter to match this candidate to the associated position opening. Typically this would help to explain why the candidate was matched to the job.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="candidateAppliedIndicator" type="hros:IndicatorType">
        <xs:annotation>
          <xs:documentation>An indicator that the candidate applied to this associated position.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="SearchCriterionType">
    <xs:annotation>
      <xs:documentation>OLD DESCRIPTION: Actual search criteria, TODO: preliminary definition</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element minOccurs="1" maxOccurs="1" name="criterionName">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Name/key of search criterion</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="1" maxOccurs="1" name="criterionValues" type="hros:StringTypeArray">
        <xs:annotation>
          <xs:documentation>(allowed) Values of search criterion</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="weight" />
      <xs:element minOccurs="0" maxOccurs="1" name="comparativeOperator">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Additional comparative operator - related to the criterion value; if not set: 'EqualTo' is assumed by default</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string">
            <xs:enumeration value="EqualTo" />
            <xs:enumeration value="LessThanOrEqualTo" />
            <xs:enumeration value="GreaterThanOrEqualTo" />
            <xs:enumeration value="LessThan" />
            <xs:enumeration value="GreaterThan" />
            <xs:enumeration value="NotEqualTo" />
            <xs:enumeration value="NotLessThan" />
            <xs:enumeration value="NotGreaterThan" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="searchString">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Search string for text/fulltext search</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="searchContext">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Search context for text/fulltext search</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="SearchResultType">
    <xs:annotation>
      <xs:documentation>OLD DESCRIPTION: Actual search result, TODO: preliminary definition</xs:documentation>
