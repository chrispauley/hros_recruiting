  <xs:complexType name="RefereeType">
    <xs:annotation>
      <xs:documentation>OLD: A person who may be referred to for information or guidance on the character or other qualities of someone, spec. of an applicant for employment, for an academic or other award, or the like.</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element minOccurs="0" maxOccurs="1" name="refereeId" type="hros:IdentifierType" />
      <xs:element minOccurs="1" maxOccurs="1" name="personName" type="hros:PersonNameType" />
      <xs:element minOccurs="0" maxOccurs="1" name="positionTitle">
        <xs:simpleType>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="organizationName">
        <xs:simpleType>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="communication" type="hros:CommunicationType" />
      <xs:element minOccurs="0" maxOccurs="1" name="yearsKnown">
        <xs:simpleType>
          <xs:restriction base="xs:decimal" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="relationship" type="hros:RelationshipTypeCodeList" />
      <xs:element minOccurs="0" maxOccurs="1" name="attachmentReferences">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:AttachmentReferenceType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="comments">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:CommentType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:complexType name="CandidateType">
    <xs:annotation>
      <xs:documentation>A person who is requesting consideration or is being considered for a current or future position at a company</xs:documentation>
    </xs:annotation>
    <xs:all>
      <xs:element minOccurs="0" maxOccurs="1" name="id" type="hros:IdentifierType">
        <xs:annotation>
          <xs:documentation>Unique identifier of candidate in the originating system. This ID is typically used to facility data interchange, and is typically assigned by the owner of the candidate.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="alternateIds">
        <xs:complexType>
          <xs:annotation>
            <xs:documentation>A set of other identifiers for the candidate. These identifiers are typically assigned by secondary or third party systems involved in data interchange.</xs:documentation>
          </xs:annotation>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:IdentifierType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="uri">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>A URI (unique resource indicator) to more information on the candidate on the internet. This could include a public profile URL, an online resume, or a person website</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:anyURI" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="distributionGuidelines" type="hros:DistributionGuidelinesType">
        <xs:annotation>
          <xs:documentation>Instructions on how this candidate should be distributed to third parties.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="person" type="hros:CandidatePersonType">
        <xs:annotation>
          <xs:documentation>Information to identify the person, including name, communcation, demographic details and other identifiers.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="1" name="profiles">
        <xs:complexType>
          <xs:annotation>
            <xs:documentation>Information on the profiles of a candidate. A typical candidate will have one profile, but additional profiles can be included in difference languages, presentations, or industry focus.</xs:documentation>
          </xs:annotation>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="hros:CandidateProfileType" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>
  <xs:element name="Candidate" type="hros:CandidateType" />
</xs:schema>