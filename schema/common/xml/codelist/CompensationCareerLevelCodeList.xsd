  <xs:simpleType name="CompensationCareerLevelCodeList">
    <xs:annotation>
      <xs:documentation>A set of codes classifying the compensation career level codes.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Entry level" />
      <xs:enumeration value="Some relevant experience" />
      <xs:enumeration value="Experienced" />
      <xs:enumeration value="Senior level" />
      <xs:enumeration value="Mastery level" />
      <xs:enumeration value="Industry expert" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="CompensationCareerLevelCode" type="hros:CompensationCareerLevelCodeList" />
</xs:schema>