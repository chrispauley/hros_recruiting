    <xs:annotation>
      <xs:documentation>An enumeration of the numeric scoring scheme.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Percentile" />
      <xs:enumeration value="RawScore" />
      <xs:enumeration value="Stanine" />
      <xs:enumeration value="Sten" />
      <xs:enumeration value="T-score" />
      <xs:enumeration value="Z-score" />
      <xs:enumeration value="IntelligenceQuotient" />
      <xs:enumeration value="Scaled" />
