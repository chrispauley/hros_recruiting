<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:hros="http://hropenstandards.org/xml/" targetNamespace="http://hropenstandards.org/xml/" version="4.0.1" id="PercentType" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:simpleType name="PercentType">
    <xs:annotation>
      <xs:documentation>A percent is a value representing a fraction of one hundred, expressed as a quotient.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal" />
  </xs:simpleType>
  <xs:element name="Percent" type="hros:PercentType" />
</xs:schema>