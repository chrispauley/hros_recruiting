<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:hros="http://hropenstandards.org/xml/" targetNamespace="http://hropenstandards.org/xml/" version="4.0.1" id="YearType" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:simpleType name="YearType">
    <xs:restriction base="xs:string">
      <xs:pattern value="[1-9]\d{3}" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="Year" type="hros:YearType" />
</xs:schema>